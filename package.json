{
  "name": "Globaltokencore",
  "description": "Globaltoken Library",
  "version": "0.1.39",
  "author": {
    "name": "globaltokendevteam",
    "email": "admin@globaltoken.org"
  },
  "contributors": [
    {
      "name": "FlappyDEV",
      "email": "dev@goflap.io"
    },
    {
      "name": "pembo210",
      "email": "pembo210@gmail.com"
    },
	{
      "name": "globaltokendevteam",
      "email": "admin@globaltoken.org"
    }
  ],
  "keywords": [
    "globaltoken",
    "glt",
    "satoshi",
	"globaltokshi",
    "money",
    "currency",
    "virtual"
  ],
  "main": "bitcore.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/globaltoken/Globaltokencore.git"
  },
  "scripts": {
    "install": "node-gyp rebuild",
    "test": "node browser/build.js -a && node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --reporter spec test",
    "prepublish": "node browser/build.js -a"
  },
  "dependencies": {
    "asn1.js": "*",
    "async": "*",
    "bignum": "*",
    "bindings": "*",
    "binary": "*",
    "bn.js": "*",
    "brfs": "*",
    "bufferput": "git://github.com/bitpay/node-bufferput.git",
    "buffers": "*",
    "buffertools": "*",
    "browserify-buffertools": "git://github.com/maraoz/browserify-buffertools.git",
    "elliptic": "*",
    "hash.js": "*",
    "jsrsasign": "*",
    "preconditions": "*"",
    "protobufjs": "*",
    "sjcl": "*",
    "socks5-client": "*""
  },
  "testling": {
    "harness": "mocha-bdd",
    "html": "test/index-testling.html",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "browser": {
    "bignum": "./lib/browser/Bignum.js",
    "buffertools": "browserify-buffertools",
    "./lib/Key.js": "./lib/browser/Key.js",
    "./lib/Point.js": "./lib/browser/Point.js",
    "./lib/ECIES.js": "./lib/browser/ECIES.js",
    "./lib/SecureRandom.js": "./lib/browser/SecureRandom.js",
    "./lib/PayPro.js": "./lib/browser/PayPro.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.4.0"
  },
  "devDependencies": {
    "browserify": "*",
    "chai": "*",
    "commander": "*",
    "coveralls": "*",
    "event-stream": "*",
    "express": "*",
    "grunt-browserify": "*",
    "grunt-contrib-watch": "*",
    "grunt-markdown": "*",
    "grunt-mocha-test": "*",
    "grunt-shell": "*",
    "gulp": "*",
    "gulp-concat": "*",
    "istanbul": "*",
    "mocha": "*",
    "optimist": "*",
    "request": "*",
    "sinon": "*",
    "uglifyify": "*"
  }
}
